@model IEnumerable<Memory>

@{
    ViewBag.Title = "Overview";
}

<h2>Overview</h2>


<table class="table table-hover">
    <thead>
        <tr>
            <th>Key</th>
            <th>Value</th>
            <th>Category</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var mem in Model) {
                <tr>
                    <td>@mem.Key</td>
                    <td>@mem.Value</td>
                    <td>@mem.Category</td>
                    @*Actions*@
                    <td>
                        @Html.ActionLink("[Edit]", "Change", new { id = mem.Id })
                        @Html.ActionLink("[Delete]", "RequestDelete", new { id = mem.Id })
                    </td>
                </tr>
            }
        }
    </tbody>
</table>

<div class="row">
    @Html.ActionLink("New", "New")
</div>

